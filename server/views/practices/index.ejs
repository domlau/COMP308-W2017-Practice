    <% include ../partials/header.ejs %>
    
    <!-- ABOUT CONTENT -->
    <div class="container">
      <div class="row">
        <div class="col-md-offset-3 col-md-6">
          <h1><%= title %></h1>
          <p>Welcome to <%= title %></p>
          <br>
          <h3>List of names</h3>
          <table class="table table-striped table-hover table-bordered">
            <tr>
              <th>Name</th>
              <th>Email</th>
              <th>Cost</th>
              <th></th>
              <th></th>
            </tr>
            <% for (let count = 0; count < practice.length; count++) { %>
            <tr> <!-- Row Template -->
              <td><%= practice[count].name %></td>
              <td><%= practice[count].email %></td>
              <td><%= practice[count].cost %></td>
              <td class="text-center"><a class="btn btn-primary" href="/practices/<%= practice[count]._id %>" role="button"><i class="fa fa-pencil-square-o"></i> Edit</a></td>
              <td class="text-center"><a class="btn btn-danger" href="#" role="button"><i class= "fa fa-trash-o"></i> Delete</a></td>
            </tr>
             <% } %>
          </table>
        </div>
      </div>
    </div>

    <% include ../partials/footer.ejs %>